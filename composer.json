{
    "name": "planethoster/planethoster-api",
    "description": "PHP library for PlanetHoster API",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "keywords": ["PlanetHoster", "API", "Domain", "Hosting"],
    "homepage": "https://github.com/PlanetHoster/planethoster-php",
    "require": {
        "php": "^7.3 || ^8.0",
        "guzzlehttp/guzzle": "~7.5.0"
    },
    "config": {
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "psr-4": {
            "PlanetHoster\\": "src/"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "7.2.0"
    }
}
